{
    "name": "alexander-levitsky/laravel-telegram-bot",
    "description": "laravel telegram bot api",
    "keywords": ["laravel", "php"],
    "license": "GPL-3.0-only",
    "homepage": "https://github.com/alexander-levitsky/laravel-telegram-bot",
    "authors": [
        {
            "name": "Alexander Levitsky",
            "email": "piro@list.ru",
            "role": "Developer"
        }
    ],
    "version": "1.0.0",
    "require": {
        "illuminate/console": "^6.3|^7.0|^8.0|^9.0",
        "illuminate/http": "^6.3|^7.0|^8.0|^9.0",
        "illuminate/queue": "^6.3|^7.0|^8.0|^9.0",
        "illuminate/routing": "^6.3|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.3|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.21"
    },
    "autoload": {
        "psr-4": {
            "Piro\\TelegramBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Piro\\TelegramBot\\Test\\": "tests/"
        }
    },


    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Piro\\TelegramBot\\TelegramServiceProvider"
            ]
        }
    }
}
